{
    "source": {
        "include": ["src", "package.json", "utils.js", "index.js", "README.md"],
        "includePattern": "\\.js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true,
        "useLongnameInNav": false,
        "showInheritedInNav": true
    },
    "opts": {
        "private": true,
        "recurse": true,
        "template": "node_modules/minami"
    }
}
